    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-putout": "^22.10.0",
    "eslint-plugin-require-explicit-generics": "1.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
