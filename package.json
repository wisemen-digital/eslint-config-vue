{
  "name": "@wisemen/eslint-config-vue",
  "type": "module",
  "version": "1.6.0.beta.0",
  "description": "Eslint config for Vue.js projects",
  "author": "Wisemen",
  "license": "ISC",
  "main": "./src/eslintConfig.js",
  "scripts": {
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm publish --access public",
    "eslint": "eslint . --fix",
    "lint": "eslint .",
    "eslint-stats": "eslint . --stats -f json",
    "test": "ts-node createFiles.ts",
    "pack": "pnpm pack"
  },
  "peerDependencies": {
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.14.1",
    "@eslint/eslintrc": "3.3.1",
    "@intlify/eslint-plugin-vue-i18n": "4.0.1",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/eslint-plugin": "^1.2.2",
    "eslint": "9.28.0",
    "eslint-plugin-better-tailwindcss": "^3.1.0",
    "eslint-plugin-check-file": "3.3.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-newline-destructuring": "^1.2.2",
    "eslint-plugin-path": "2.0.3",
    "eslint-plugin-perfectionist": "4.14.0",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-readable-tailwind": "^2.0.0",
    "eslint-plugin-require-explicit-generics": "1.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^2.2.0",
    "globals": "^16.2.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.10",
    "ts-node": "10.9.2",
    "tsx": "^4.20.2",
    "vitest": "^3.2.3"
  }
}
